<AppManifest version="1">
    <!-- Defines the app's identity (name, displayname, etc) -->
    <Identity>
        <Name>snakeware.boot2</Name>
        <Version>1.0.0</Version>
    </Identity>

    <Modules>
        <Module path="__init__.py"/>
    </Modules>

    <Provides>
        <App appid="boot2" />
    </Provides>

    <Permissions>
        <Install permission="fs.userdata.access"/>
    </Permissions>

    <Entrypoints>
        <ClassEntry id="boot" class="snakeware.boot2.Boot2"/>
    </Entrypoints>
</AppManifest>